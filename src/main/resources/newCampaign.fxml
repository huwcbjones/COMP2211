                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="clickLogText" editable="false" promptText="clicks.csv" GridPane.columnIndex="2" GridPane.columnSpan="4"/>
                <Button fx:id="clickLogBrowseButton" mnemonicParsing="false" text="Browse" GridPane.columnIndex="6" GridPane.halignment="CENTER"
