                <RowConstraints maxHeight="-Infinity" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints/>
            </rowConstraints>
            <BorderPane.margin>
                <Insets bottom="100.0" left="50.0" right="50.0" top="100.0"/>
            </BorderPane.margin>
            <children>
                <Label text="Click Log" GridPane.columnSpan="2" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
