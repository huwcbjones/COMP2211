                    </GridPane.margin>
                </Button>
                <Label text="Campaign Location" GridPane.columnSpan="2" GridPane.rowIndex="4"/>
                <TextField fx:id="campaignSaveText" editable="false" GridPane.columnIndex="2" promptText="campaign.h2" GridPane.columnSpan="4" GridPane.rowIndex="4"/>
                <Button mnemonicParsing="false" text="Browse" GridPane.columnIndex="6" GridPane.rowIndex="4" onAction="#campaignBrowseAction">
