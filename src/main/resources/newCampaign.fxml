                </Label>
                <TextField fx:id="impressionLogText" editable="false" promptText="impressions.csv" GridPane.columnIndex="2" GridPane.columnSpan="4" GridPane.rowIndex="1"/>
                <Button fx:id="impressionLogBrowseButton" mnemonicParsing="false" text="Browse" GridPane.columnIndex="6" GridPane.halignment="CENTER"
