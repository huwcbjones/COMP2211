<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane focusTraversable="true" maxHeight="150" maxWidth="250" minHeight="150" minWidth="250" prefHeight="150.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="t16.controller.Export">
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="15.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="15.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <BorderPane.margin>
                <Insets bottom="100.0" left="50.0" right="50.0" top="100.0" />
            </BorderPane.margin>
            <children>
                <Button fx:id="saveScreenButton" mnemonicParsing="false" onAction="#saveScreen" text="Save Screen as Image" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0">
                    <GridPane.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="pdfScreenButton" mnemonicParsing="false" onAction="#pdfScreen" text="Save Screen as PDF" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="printScreenButton" mnemonicParsing="false" onAction="#printScreen" text="Print Screenshot" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </GridPane.margin>
                </Button>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" prefWidth="109.0" cancelButton="true">
                    <GridPane.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </GridPane.margin>
                </Button>
            </children>
        </GridPane>
    </center>
</BorderPane>